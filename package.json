{
  "name": "squaredb",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npx tsc",
    "start": "node --require dotenv/config ./dist/src/server.js dotenv_config_path=./.env",
    "dev": "npm run clean && npx nodemon",
    "tsc-watch": "npx tsc --watch",
    "test": "jest --maxWorkers=100% --collect-coverage",
    "test:watch": "jest --watch --collect-coverage",
    "test:clearCache": "jest --clearCache",
    "localdev": "nodemon src/server.js --env-file=.env",
    "prisma:generate": "prisma generate --schema=./src/model",
    "prisma:db:pull": "prisma db pull --schema=./src/model",
    "prisma:db:push": "prisma db push --schema=./src/model",
    "prisma:db:migrate": "prisma migrate dev --schema=./src/model"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@openfeature/core": "^1.3.0",
    "@openfeature/flagd-provider": "^0.13.1",
    "@openfeature/js-sdk": "^1.3.0",
    "@prisma/client": "^5.18.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "luxon": "^3.5.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.12.0",
    "ulid": "^2.3.0",
    "winston": "^3.13.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.1.0",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "^8.11.6",
    "eslint": "^9.8.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
