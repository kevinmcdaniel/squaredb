{
  "name": "enabledb",
  "version": "0.0.1",
  "description": "enabledb api",
  "main": "src/server.ts",
  "directories": {
    "test": "test"
  },
  "prisma": {
    "schema": "./src/model"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && npx tsc",
    "start": "node --require dotenv/config ./dist/src/server.js dotenv_config_path=./.env",
    "start:prod": "npm run clean && npx tsc && npm run start",
    "dev": "npm run clean && npx nodemon",
    "start-app": "npm run clean && npm run build && npm start",
    "tsc-watch": "npx tsc --watch",
    "test": "jest --maxWorkers=100% --collect-coverage",
    "test:watch": "jest --watch --collect-coverage",
    "test:clearCache": "jest --clearCache",
    "prisma:generate": "prisma generate --schema=./src/model",
    "prisma:db:pull": "prisma db pull --schema=./src/model",
    "prisma:db:push": "prisma db push --schema=./src/model",
    "prisma:db:migrate": "prisma migrate dev --schema=./src/model"
  },
  "author": "Tejashree Thaker<tejashree.patil@nviz.com>,Chandan Pasunoori<chandan.pasunoori@nviz.com>",
  "license": "ISC",
  "devDependencies": {
    "@jest/types": "^29.6.3",
    "@openfeature/js-sdk": "^1.6.1",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.11",
    "@types/pg": "^8.11.6",
    "axios": "^1.7.2",
    "chai": "^4.2.0",
    "chai-http": "^4.4.0",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.7",
    "lodash": "^4.17.21",
    "mocha": "^10.4.0",
    "nodemon": "^3.1.4",
    "ts-jest": "^29.2.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "engines": {
    "node": ">=22.2.0",
    "npm": ">=10.7.0"
  },
  "dependencies": {
    "@openfeature/core": "^1.3.0",
    "@openfeature/flagd-provider": "^0.13.1",
    "@openfeature/server-sdk": "^1.15.0",
    "@prisma/adapter-pg": "^5.15.1",
    "@prisma/client": "^5.15.1",
    "axios": "^1.7.2",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.4.5",
    "fast-csv": "^5.0.1",
    "fs": "^0.0.1-security",
    "htmlparser2": "^9.1.0",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.4.4",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.12.0",
    "prisma": "^5.15.1",
    "react-csv-reader": "^4.0.0",
    "readline": "^1.3.0",
    "ulid": "^2.3.0",
    "winston": "^3.13.1",
    "xml2js": "^0.6.2"
  }
}
